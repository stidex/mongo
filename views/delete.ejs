    <section>
      <div class="principal">
        <p><span>IMMERSIVE</span><span>EXPERIENCES</span><span>THAT DELIVER</span></p>
      </div>
    </section>

    <section>
      <div class="imagen">
        <img src="fotos/fondo.jpg">
